{
  "name": "@lmtnolimit/react-iframe",
  "author": "Lâm Minh Thành",
  "version": "1.0.3",
  "type": "module",
  "keywords": [
    "React",
    "react-iframe",
    "Iframe",
    "component",
    "IframeWrapper"
  ],
  "license": "MIT",
  "scripts": {
    "dev": "vite",
    "build": "tsc -p tsconfig.prod.json && vite build",
    "dev-build": "tsc -p tsconfig.json && vite build --watch",
    "preview": "vite preview",
    "husky": "husky",
    "prettier": "prettier --write **/*.{ts,tsx,d.ts,json}"
  },
  "devDependencies": {
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "@swc/core": "^1.3.26",
    "@types/react": "^18.0.25",
    "@types/react-dom": "^18.0.9",
    "@vitejs/plugin-react-swc": "^3.0.1",
    "eslint": "^8.31.0",
    "eslint-config-prettier": "^8.6.0",
    "eslint-plugin-prettier": "^4.2.1",
    "husky": "^8.0.3",
    "lint-staged": "^13.1.0",
    "prettier": "^2.8.1",
    "tslib": "^2.4.1",
    "typescript": "^4.9.4",
    "vite": "^4.0.3",
    "vite-plugin-dts": "^1.7.1"
  },
  "peerDependencies": {
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "files": [
    "dist"
  ],
  "main": "./dist/elements.es.js",
  "module": "./dist/elements.umd.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/elements.es.js",
      "require": "./dist/elements.umd.js"
    }
  },
  "lint-staged": {
    "**/*.{ts,tsx,d.ts,json,html}": [
      "prettier --write"
    ]
  },
  "description": "A React component that can wrap your components into an iframe",
  "dependencies": {},
  "repository": {
    "type": "git",
    "url": "git+https://github.com/lmtNoLimit/react-iframe.git"
  },
  "bugs": {
    "url": "https://github.com/lmtNoLimit/react-iframe/issues"
  },
  "homepage": "https://github.com/lmtNoLimit/react-iframe#readme"
}
